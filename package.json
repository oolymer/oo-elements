{
  "name": "oo-elements",
  "description": "",
  "version": "0.0.0",
  "author": "Benjamin Gudehus <hastebrot@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/oolymer/oo-elements.git"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "bower": "1.8.4",
    "browser-sync": "2.24.4",
    "npm-delay": "1.0.4",
    "npm-run-all": "4.1.3",
    "polyserve": "^0.27.11"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "./node_modules/.bin/npm-run-all --parallel serve dev:serve-watch",
    "dev:serve-watch": "./node_modules/.bin/npm-run-all --serial util:delay serve:watch",
    "install:bower": "./node_modules/.bin/bower install --force-latest",
    "serve": "./node_modules/.bin/polyserve --port $npm_package_config_portServe",
    "serve:watch": "./node_modules/.bin/browser-sync start --port $npm_package_config_portServeWatch --proxy \"localhost:$npm_package_config_portServe\" --no-ui --files '*.js, *.html, demo/**/*.html, src/**/*.html, test/**/*.html' --startPath \"/components/$npm_package_name/\"",
    "util:delay": "node ./node_modules/.bin/npm-delay 1000"
  },
  "config": {
    "portServe": "8500",
    "portServeWatch": "8510"
  }
}
